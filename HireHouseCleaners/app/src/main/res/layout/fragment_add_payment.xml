<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddPaymentFragment">

    <TextView
        android:id="@+id/n_rooms"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/h_type_of_floor"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n_koridows" />

    <TextView
        android:id="@+id/n_koridows"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n_garages" />

    <TextView
        android:id="@+id/n_garages"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n_kitchens" />

    <TextView
        android:id="@+id/n_kitchens"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n_main_floor" />

    <TextView
        android:id="@+id/n_main_floor"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n_bathrooms" />

    <TextView
        android:id="@+id/n_bathrooms"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n_rooms" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="33dp"
        android:text="Customer User Name :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text="Order Posted Date    :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="217dp"
        android:layout_height="61dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="8dp"
        android:text="Add Payment"
        android:textSize="35dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="230dp"
        android:layout_height="34dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:text="House Informations"
        android:textSize="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c_location" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text="Customer Location  :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="22dp"
        android:text="Num of Rooms :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="1dp"
        android:text="Type of Floor :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text="Num of Koridows:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text="Num of Garages :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text="Num of Kitchens :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text=" Num of Main Floores :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="144dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:text="Num of Bathrooms :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/c_username"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="33dp"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/c_location"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c_posted_date" />

    <TextView
        android:id="@+id/c_posted_date"
        android:layout_width="146dp"
        android:layout_height="27dp"
        android:layout_marginEnd="91dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c_username" />

    <Button
        android:id="@+id/home_images_button"
        android:layout_width="368dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="10dp"
        android:text="Home Images"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/h_type_of_floor" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="147dp"
        android:layout_height="29dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="18dp"
        android:text="Add Payment :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_images_button" />

    <EditText
        android:id="@+id/house_payment_input"
        android:layout_width="211dp"
        android:layout_height="47dp"
        android:layout_marginStart="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/home_images_button" />

    <Button
        android:id="@+id/submit_payment"
        android:layout_width="366dp"
        android:layout_height="40dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="31dp"
        android:text="Submit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <Button
        android:id="@+id/feedback_clearner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:background="@android:color/transparent"
        android:text="Feedback"
        android:textColor="#126BF1"
        android:textSize="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>